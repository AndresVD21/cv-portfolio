{
  "projectType": "library",
  "sourceRoot": "libs/app-config/src",
  "prefix": "cv-portfolio",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/app-config"],
      "options": {
        "jestConfig": "libs/app-config/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/app-config/**/*.ts",
          "libs/app-config/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:config"]
}
